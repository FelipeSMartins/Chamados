@model WebApplication1.DepartamentoAtendente

@{
    ViewData["Title"] = "Delete";
}

<h3>Tem certeza de que deseja excluir isso?</h3>
<div>
    <h4>Departamento - Atendente</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Atendente
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DaAtendente.AteId)
        </dd>
        <dt class="col-sm-2">
            Departamento
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DaDepartamento.DepEmail)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="DaDepartamentoid" />
        <input type="hidden" asp-for="DaAtendenteid" />
        <input type="submit" value="Deletar" class="btn btn-danger" /> |
        <a asp-action="Index">Voltar</a>
    </form>
</div>
