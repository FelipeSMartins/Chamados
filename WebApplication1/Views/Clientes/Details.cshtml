@model WebApplication1.Cliente

@{
    ViewData["Title"] = "Details";
}

<div>
    <h4>Cliente</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            CNPJ
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CliOrganizacao.OrgCnpj)
        </dd>
        <dt class = "col-sm-2">
            Email
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CliPessoa.PesEmail)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.CliId">Editar</a> |
    <a asp-action="Index">Voltar</a>
</div>
