@model WebApplication1.Cliente

@{
    ViewData["Title"] = "Delete";
}

<h3>Tem certeza de que deseja excluir isso?</h3>
<div>
    <h4>Cliente</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            CNPJ
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CliOrganizacao.OrgCnpj)
        </dd>
        <dt class="col-sm-2">
            Email
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CliPessoa.PesEmail)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="CliId" />
        <input type="submit" value="Deletar" class="btn btn-danger" /> |
        <a asp-action="Index">Voltar</a>
    </form>
</div>
