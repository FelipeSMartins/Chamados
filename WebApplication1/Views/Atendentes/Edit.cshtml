@model WebApplication1.Atendente

@{
    ViewData["Title"] = "Edit";
}

<h4>Atendente</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="AteId" />
            <div class="form-group">
                <label asp-for="AtePessoaid" class="control-label">Email</label>
                <select asp-for="AtePessoaid" class="form-control" asp-items="ViewBag.AtePessoaid"></select>
                <span asp-validation-for="AtePessoaid" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Salvar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Voltar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
