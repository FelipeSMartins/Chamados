@model WebApplication1.StatusChamado

@{
    ViewData["Title"] = "Create";
}

<h4>StatusChamado</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="SchaNome" class="control-label">Status</label>
                <input asp-for="SchaNome" class="form-control" />
                <span asp-validation-for="SchaNome" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Voltar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
