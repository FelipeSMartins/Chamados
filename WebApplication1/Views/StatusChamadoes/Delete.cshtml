@model WebApplication1.StatusChamado

@{
    ViewData["Title"] = "Delete";
}

<h3>Tem certeza de que deseja excluir isso?</h3>
<div>
    <h4>StatusChamado</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Status
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.SchaNome)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="SchaId" />
        <input type="submit" value="Deletar" class="btn btn-danger" /> |
        <a asp-action="Index">Voltar</a>
    </form>
</div>
